import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
       int[] org={4,7,8,9};
       int[] cp=new int[org.length];
       for(int i=0;i<org.length;i++){
           cp[i]=org[i];
       }
       System.out.println("org array: "+Arrays.toString(org));
       System.out.println("copied array: "+Arrays.toString(cp));

    }
}
